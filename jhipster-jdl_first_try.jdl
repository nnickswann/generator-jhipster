
entity People {
	id UUID,
    address String,
    email String,
    password String,
    name String
}

    /**
    *quantity можеть быть Integer, в зависимости от задачи
    */
entity Orders{
	id UUID,
    userId UUID,
    productId UUID,
    total Double,
    quantity Double
}    

    /**
    *category можеть быть enum
    */
entity Products {
	id UUID,
    title String,
    category String,
    price Double
}